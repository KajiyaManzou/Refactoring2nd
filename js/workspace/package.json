{
    "name": "javascript-refactoring",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "start": "babel src --out-dir dist"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5"
    },
    "devDependencies": {
        "babel-jest": "^24.8.0",
        "babel-polyfill": "^6.26.0",
        "jest": "^24.8.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "test",
        "testRegex": ".*\\.spec\\.js$",
        "transform": {
            "^.+\\.(t|j)s$": "babel-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}